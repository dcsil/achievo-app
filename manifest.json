{
  "name": "Achievo",
  "description": "A productivity extension to help you stay focused and achieve your goals.",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["sidePanel", "notifications", "storage"],
  "background": {
    "service_worker": "service_worker.js"
  },
  "side_panel": {
    "default_path": "landing.html"
  },
  "action": {
    "default_title": "Achievo",
    "default_icon": "achievo_thumbnail.png"
  },
  "icons": {
    "16": "achievo_thumbnail.png",
    "48": "achievo_thumbnail.png",
    "128": "achievo_thumbnail.png"
  }
}
